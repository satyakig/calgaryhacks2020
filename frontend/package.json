{
  "name": "calgaryhacks2020",
  "description": "Calgary Hacks 2020",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=12.13",
    "npm": ">=6.12"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "pre-commit": "lint:staged",
  "lint-staged": {
    "*.{js,jsx},!functions/*": [
      "npm run lint:fix"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint:js": "eslint --cache src --ext .ts --ext .tsx --ext .js --ext .jsx",
    "lint:fix": "npm run lint:js -- --fix",
    "lint": "npm run lint:js",
    "lint:staged": "lint-staged",
    "compile-sass": "node-sass src/assets/sass/light-bootstrap-dashboard-react.scss src/assets/css/light-bootstrap-dashboard-react.css",
    "minify-sass": "node-sass src/assets/sass/light-bootstrap-dashboard-react.scss src/assets/css/light-bootstrap-dashboard-react.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/sass/light-bootstrap-dashboard-react.scss src/assets/css/light-bootstrap-dashboard-react.css --source-map true"
  },
  "dependencies": {
    "bootstrap": "3.4.1",
    "chartist": "0.10.1",
    "classnames": "2.2.6",
    "core-js": "3.6.4",
    "firebase": "7.8.1",
    "node-sass": "4.12.0",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-app-polyfill": "1.0.6",
    "react-bootstrap": "0.32.4",
    "react-chartist": "0.13.3",
    "react-dom": "16.12.0",
    "react-google-maps": "9.4.5",
    "react-notification-system": "0.2.17",
    "react-redux": "7.1.3",
    "react-router": "5.0.0",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.4.0",
    "react-toggle": "4.0.2",
    "redux": "4.0.5",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "2.19.2",
    "@typescript-eslint/parser": "2.19.2",
    "@types/googlemaps": "3.30.19",
    "@types/markerclustererplus": "2.1.33",
    "@types/react": "16.8.13",
    "eslint": "6.8.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-config-react-app": "5.2.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "2.3.0",
    "firebase-tools": "7.13.0",
    "husky": "4.2.2",
    "lint-staged": "10.0.7",
    "node-sass": "4.13.1",
    "pre-commit": "1.2.2",
    "prettier": "1.19.1",
    "stylelint": "13.1.0",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-scss": "3.14.2",
    "typescript": "3.7.5"
  }
}
